<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/java10"
    tools:context="com.example.mddanish.myapplication.MainActivity">


    <TextView
        android:text="@string/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        tools:textStyle="bold|italic" />

    <TextView
        android:text="@string/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView1"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold|italic" />

    <ImageButton
        android:src="@drawable/danish"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="5dp"
        android:padding="0dp"
        android:scaleType="fitXY"
        android:id="@+id/imageButton1"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="@string/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_marginStart="20dp"
        android:layout_below="@+id/textView2"
        android:layout_toEndOf="@+id/imageButton1"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button1"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:textSize="12sp"
        style="@android:style/Widget.Button.Small"
        android:layout_above="@+id/imageButton2" />

    <ImageButton
        android:src="@drawable/me"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="5dp"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitXY"
        android:id="@+id/imageButton2"
        android:layout_below="@+id/imageButton1"
        android:layout_alignParentStart="true"
        android:layout_marginTop="2dp" />

    <TextView
        android:text="@string/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignStart="@+id/button1"
        android:id="@+id/textView5"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:id="@+id/textView6"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:id="@+id/button2"
        android:layout_above="@+id/imageButton3"
        android:textSize="12sp"
        style="@android:style/Widget.Button.Small" />

    <ImageButton
        android:src="@drawable/rahul"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="5dp"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitXY"
        android:id="@+id/imageButton3"
        android:layout_below="@+id/imageButton2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="2dp" />

    <TextView
        android:text="@string/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignStart="@+id/button2"
        android:id="@+id/textView7"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:id="@+id/textView8"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:id="@+id/button3"
        android:layout_alignBottom="@+id/imageButton3"
        android:textSize="12sp"
        style="@android:style/Widget.Button.Small" />

    <ImageButton
        android:src="@drawable/ajay"
        android:layout_height="63dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="5dp"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitXY"
        android:id="@+id/imageButton4"
        android:layout_below="@+id/imageButton3"
        android:layout_alignParentStart="true"
        android:layout_marginTop="2dp"
        android:layout_width="63dp" />

    <TextView
        android:text="@string/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignStart="@+id/button3"
        android:id="@+id/textView9"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:id="@+id/textView10"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:id="@+id/button4"
        android:layout_alignBottom="@+id/imageButton4"
        android:textSize="12sp"
        style="@android:style/Widget.Button.Small" />

    <ImageButton
        android:src="@drawable/shantanu"
        android:layout_width="63dp"
        android:layout_height="63dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="5dp"
        android:adjustViewBounds="true"
        android:padding="0dp"
        android:scaleType="fitXY"
        android:id="@+id/imageButton5"
        android:layout_below="@+id/imageButton4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="2dp" />

    <TextView
        android:text="@string/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignStart="@+id/button4"
        android:id="@+id/textView11"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:id="@+id/textView12"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:id="@+id/button5"
        android:textSize="12sp"
        style="@android:style/Widget.Button.Small"
        android:layout_alignBottom="@+id/imageButton5" />

    <TextView
        android:text="@string/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_toEndOf="@+id/button1"
        android:layout_alignBottom="@+id/button1"
        android:layout_below="@+id/textView3" />

    <TextView
        android:text="@string/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_toEndOf="@+id/button2"
        android:id="@+id/textView14"
        android:textStyle="normal|bold"
        android:textSize="18sp" />

    <TextView
        android:text="@string/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_toEndOf="@+id/button3"
        android:id="@+id/textView15"
        android:textSize="18sp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_alignEnd="@+id/textView15"
        android:id="@+id/textView16"
        android:textSize="18sp"
        tools:textStyle="bold" />

    <TextView
        android:text="@string/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_toEndOf="@+id/button5"
        android:id="@+id/textView17"
        android:textSize="18sp"
        android:textStyle="normal|bold" />





</RelativeLayout>